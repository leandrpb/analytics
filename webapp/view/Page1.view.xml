                                    <smartvariants:SmartVariantManagement showShare="true" showExecuteOnSelection="false"/>
                                </layout:content>
                            </layout:HorizontalLayout>
                        </layout:content>
                    </layout:VerticalLayout>
                </f:heading>
                <f:content>
